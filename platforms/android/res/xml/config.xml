<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dsksoft.amilcarcard" version="2.18.21" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Sim">
        <param name="android-package" value="com.pbakondy.Sim" />
    </feature>
    <feature name="Screenshot">
        <param name="android-package" value="com.darktalker.cordova.screenshot.Screenshot" />
    </feature>
    <name>SIM KEY</name>
    <description>
          Business Card App responsive for Android smart phones
    </description>
    <author email="marco@dsksoft.com" href="http://dsksoft.com">
          DSKsoft.com Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
